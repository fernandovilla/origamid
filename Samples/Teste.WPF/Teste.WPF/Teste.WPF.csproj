<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net5.0-windows</TargetFramework>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Dialogs\info.png" />
    <None Remove="folder.png" />
    <None Remove="help.png" />
    <None Remove="inventory_2-24px.png" />
    <None Remove="Menus\bold.png" />
    <None Remove="Menus\cut.png" />
    <None Remove="Menus\italic.png" />
    <None Remove="Menus\open-folder.png" />
    <None Remove="Menus\save.png" />
    <None Remove="Menus\underline.png" />
    <None Remove="newFile.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="MaterialDesignThemes" Version="4.0.0" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Dialogs\info.png" />
    <Resource Include="folder.png" />
    <Resource Include="help.png" />
    <Resource Include="inventory_2-24px.png" />
    <Resource Include="Menus\bold.png" />
    <Resource Include="Menus\cut.png" />
    <Resource Include="Menus\italic.png" />
    <Resource Include="Menus\open-folder.png" />
    <Resource Include="Menus\save.png" />
    <Resource Include="Menus\underline.png" />
    <Resource Include="newFile.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Resources.Designer.cs">
      <DesignTime>True</DesignTime>
      <AutoGen>True</AutoGen>
      <DependentUpon>Resources.resx</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <EmbeddedResource Update="Properties\Resources.resx">
      <Generator>ResXFileCodeGenerator</Generator>
      <LastGenOutput>Resources.Designer.cs</LastGenOutput>
    </EmbeddedResource>
  </ItemGroup>

  <ItemGroup>
    <None Update="Sample.Binding1">
      <Generator>MSBuild:Compile</Generator>
    </None>
  </ItemGroup>

</Project>
